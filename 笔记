#切取字段
x <- "GSM6727543_LPDAC_25_Tumor"
# 按下划线切开，得到一个列表
parts <- strsplit(x, split = "_")[[1]]
# 取第2个
print(parts[2]) 
# 输出: "LPDAC"


x <- "GSM6727543_LPDAC_25_Tumor"
# 1. 切开
parts <- strsplit(x, split = "_")[[1]]
# 此时 parts 是 c("GSM6727543", "LPDAC", "25", "Tumor")
# 2. & 3. 挑出来并拼回去
# parts[1:2] 选出了 "GSM6727543" 和 "LPDAC"
result <- paste(parts[1:2], collapse = "_")
print(result)
# 输出: "GSM6727543_LPDAC"
